{
  "name": "expert-sys",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "export NODE_ENV=prod && forever start -l forever.log --append -o ./logs/out.log -e ./logs/err.log app.js",
    "stop": "forever stop app.js",
    "restart": "forever restart -l forever.log --append -o ./logs/out.log -e ./logs/err.log app.js",
    "dev": "export NODE_ENV=dev && nodemon app.js"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "body-parser": "~1.15.2",
    "composable-middleware": "^0.3.0",
    "connect-history-api-fallback": "^1.3.0",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.1",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "express-jwt": "^5.1.0",
    "express-session": "^1.14.2",
    "jade": "~1.11.0",
    "jsonwebtoken": "^7.1.9",
    "lei-stream": "^1.3.0",
    "moment": "^2.17.1",
    "mongoose": "^4.7.0",
    "morgan": "~1.7.0",
    "nodejieba": "^2.2.4",
    "nools": "^0.4.4",
    "serve-favicon": "~2.3.0",
    "underscore": "^1.8.3",
    "xss": "^0.3.2"
  }
}
